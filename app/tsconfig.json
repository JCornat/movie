{
  "compilerOptions": {
    "target": "es2020",
    "noImplicitAny": false,
    "sourceMap": true,
    "baseUrl": ".",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "lib": [
      "esnext"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {
      "@class/*": [
        "class/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@model/*": [
        "model/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
